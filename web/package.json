{
  "description": "w3 ledgers",
  "author": "Vineel Nalla",
  "version": "0.1.0",
  "name": "w3l",
  "homepage": ".",
  "main": "app/background.js",
  "private": true,
  "scripts": {
    "run:dev": "next",
    "build": "next build",
    "start": "next start",
    "gql:gen:all": "npm run gql:gen:identity && npm run gql:gen:accounting",
    "gql:gen:identity": "graphql-codegen --config src/mfe/identity/config/codegen.yml",
    "gql:gen:accounting": "graphql-codegen --config src/mfe/accounting/config/codegen.yml",
    "postinstall": "electron-builder install-app-deps",
    "electron:dev": "concurrently \"cross-env BROWSER=none yarn run:dev\" \"wait-on http://127.0.0.1:3000 && tsc -p electron -w\" \"wait-on http://127.0.0.1:3000 && tsc -p electron && electron .\"",
    "electron:build": "yarn build && tsc -p electron && electron-builder",
    "nextron:build": "nextron build",
    "nextron:dev": "nextron dev --port 3000"
  },
  "build": {
    "extends": null,
    "files": [
      "build/**/*",
      "app"
    ],
    "directories": {
      "buildResources": "assets"
    }
  },
  "dependencies": {
    "@chakra-ui/icons": "^2.0.2",
    "@chakra-ui/react": "^2.2.1",
    "@chakra-ui/theme-tools": "^2.0.2",
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.9.0",
    "@types/electron-devtools-installer": "^2.2.2",
    "@urql/devtools": "^2.0.3",
    "@urql/exchange-auth": "^1.0.0",
    "@urql/exchange-graphcache": "^4.4.3",
    "date-fns": "^2.29.2",
    "dotenv": "^16.0.3",
    "electron-devtools-installer": "^3.2.0",
    "electron-is-dev": "^2.0.0",
    "electron-reload": "^2.0.0-alpha.1",
    "electron-serve": "^1.1.0",
    "electron-store": "^8.1.0",
    "formik": "^2.1.5",
    "framer-motion": "^6.3.0",
    "graphql": "^16.5.0",
    "next": "^12.2.3",
    "next-urql": "^4.0.0",
    "nextron": "^8.1.0",
    "oidc-client": "^1.11.5",
    "react": "^18.2.0",
    "react-currency-format": "^1.1.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.3.1",
    "react-is": "^18.2.0",
    "react-router-dom": "^6.4.2",
    "reflect-metadata": "^0.1.13",
    "typedi": "^0.10.0",
    "urql": "^3.0.3",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.11.3",
    "@graphql-codegen/typescript": "2.7.2",
    "@graphql-codegen/typescript-operations": "2.5.2",
    "@graphql-codegen/typescript-urql": "^3.6.3",
    "@types/dotenv-safe": "^8.1.2",
    "@types/node": "^18.0.0",
    "@types/react": "^18.0.0",
    "@types/react-currency-format": "^1.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/react-router-dom": "^5.3.3",
    "concurrently": "^7.4.0",
    "cross-env": "^7.0.3",
    "electron": "^21.1.1",
    "electron-builder": "^23.6.0",
    "typescript": "^4.7.2",
    "wait-on": "^6.0.1"
  }
}
