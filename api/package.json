    "mig:gen:accounting": "typeorm migration:generate -c accounting -d src/migrations/accounting -f ormconfig.json -n ",
    "mig:gen:dacchain": "typeorm migration:generate -c dacchain -d src/migrations/dacchain -f ormconfig.json -n "
